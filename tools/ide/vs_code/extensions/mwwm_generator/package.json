{
	"name": "mwwm-generator",
	"publisher": "SurfGear",
	"displayName": "mwwm-generator",
	"description": "Extension helps you to create mwwm-widgets",
	"repository": "https://github.com/surfstudio/SurfGear",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mwwm-generator.create-mwwm-widget",
				"title": "Create Mwwm Widget"
			},
			{
				"command": "mwwm-generator.create-surf-mwwm-widget",
				"title": "Create Surf-Mwwm Widget"
			},
			{
				"command": "mwwm-generator.create-surf-mwwm-screen",
				"title": "Create Surf-Mwwm Screen"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "mwwm-generator.create-mwwm-widget",
					"group": "creating",
					"when": "true"
				},
				{
					"command": "mwwm-generator.create-surf-mwwm-widget",
					"group": "creating",
					"when": "true"
				},
				{
					"command": "mwwm-generator.create-surf-mwwm-screen",
					"group": "creating",
					"when": "true"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^8.2.3",
		"@types/node": "^16.3.1",
		"@types/vscode": "^1.58.0",
		"@typescript-eslint/eslint-plugin": "^4.29.2",
		"@typescript-eslint/parser": "^4.28.2",
		"eslint": "^7.30.0",
		"glob": "^7.1.6",
		"mocha": "^9.0.2",
		"typescript": "^4.3.5",
		"vscode-test": "^1.4.0"
	}
}
